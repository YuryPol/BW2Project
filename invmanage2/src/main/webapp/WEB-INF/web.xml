<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <servlet>
      <servlet-name>gcs</servlet-name>
      <servlet-class>com.bwing.invmanage2.FileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>gcs</servlet-name>
      <url-pattern>/gcs</url-pattern>
  </servlet-mapping>  
  
  <servlet>
      <servlet-name>load</servlet-name>
      <servlet-class>com.bwing.invmanage2.LoadInventory</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>load</servlet-name>
      <url-pattern>/load</url-pattern>
  </servlet-mapping>  
  <servlet>
      <servlet-name>loadwork</servlet-name>
      <servlet-class>com.bwing.invmanage2.Loadworker</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>loadwork</servlet-name>
      <url-pattern>/loadwork</url-pattern>
  </servlet-mapping>  
  
  <servlet>
      <servlet-name>startsimulate</servlet-name>
      <servlet-class>com.bwing.invmanage2.StartSimulation</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>startsimulate</servlet-name>
      <url-pattern>/startsimulate</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>simulate</servlet-name>
      <servlet-class>com.bwing.invmanage2.Simulation</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>simulate</servlet-name>
      <url-pattern>/simulate</url-pattern>
  </servlet-mapping>
    
	<error-page>
	    <exception-type>javax.servlet.ServletException</exception-type>
	    <location>/errors/errorpage.jsp</location>
	</error-page>	
	<error-page>
	    <exception-type>java.io.IOException</exception-type>
	    <location>/errors/errorpage.jsp</location>
	</error-page>  
    <error-page>
        <exception-type>java.io.SQLException</exception-type>
        <location>/errors/errorpage.jsp</location>
    </error-page>  
  
  <welcome-file-list>
    <welcome-file>Start.jsp</welcome-file>
  </welcome-file-list>
<!-- [START Objectify] -->
<filter>
  <filter-name>ObjectifyFilter</filter-name>
  <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
</filter>
<filter-mapping>
  <filter-name>ObjectifyFilter</filter-name>
  <url-pattern>/*</url-pattern>
</filter-mapping>
<listener>
  <listener-class>com.bwing.invmanage2.OfyHelper</listener-class>
</listener>
<!-- [END Objectify] -->
</web-app>
